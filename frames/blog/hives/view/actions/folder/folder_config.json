{
	"name":        "blog_folder",
	"layout_name": "hiveblog",
	"routes":      {
		"get": [
			"/blog_folder/:folder",
            "/blog_articles/"
		]
	},

	"static": {
		"js":  "/js/blog_folders"
	},

	"javascript": [
		{
			"url":     "/js/blog_folders/vendor/moment.min.js",
			"context": "head"
		},
		{
			"url":     "/js/blog_folders/vendor/underscore-min.js",
			"context": "head"
		},
		{
			"url":     "/js/blog_folders/vendor/jquery-1.10.1.min.js",
			"context": "head"
		},
		{
			"url":     "/js/blog_folders/vendor/angular/angular.js",
			"context": "head"
		},
		{
			"url":     "/js/blog_folders/vendor/angular/angular-resource.min.js",
			"context": "head"
		},
		{
            "url":     "/js/blog_folders/vendor/angular/ng-grid-2.0.7.debug.js",
			"context": "head",
            "requires": ["angular"]
		},
		{
			"url":      "/js/blog_folders/controller.js",
			"context":  "head",
			"requires": ["angular"]
		}
	]
}